<main role="main" id="content">
	<section id="left-pane" class="pane">
		<aside id="portrait">
		</aside>
		<div id="level-text">
			<% @level_pages.each do |page| %>
			<p> <%= page.content.html_safe %> </p>
			<% end %>
			
			<%= link_to "Congrats you dont suck! Move on up.",level_path(@level.next_level) if @level.next_level %>
		</div>
	</section>
	<section id="repl">
		<div id="output">
			<pre id="output-text"></pre>
		</div>
		<div id="command-input" class="pane">
			<textarea id="sql-command" autofocus></textarea>
			<button type="button" id="execute">EXECUTE</button>
		</div>
	</section>
</main>