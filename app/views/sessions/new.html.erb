<h1>Login</h1>
<%= flash[:notice] %>

<%= form_tag(sessions_path) do %>
  <p>  
    <%= label_tag "user_name","Username:" %>
    <%= text_field_tag "user[user_name]" %>
  </p>

  <p>  
    <%= label_tag "password" %>
    <%= password_field_tag "user[password]" %>
  </p>

  <%= submit_tag "Login" %>

<% end %>